<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<?import java.net.URL?>
<VBox spacing="10" alignment="center" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coachup.controller.WelcomeAdminController">
    <stylesheets>
        <URL value="@/css/application.css" />
    </stylesheets>
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>
    <Label style="-fx-font-size: 20px; -fx-font-weight: bold;" text="Welcome Admin"/>

    <Button text="Liste des utilisateurs" onAction="#userListButtonAction"/>
    <Button text="Approbation des coachs" onAction="#coachApprovalListButtonAction"/>
    <Button text="Gérer les catégories" onAction="#manageCategoriesButtonAction"/>
</VBox>
