<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="360.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="coachup.controller.WelcomeController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Button onAction="#showCalendarButtonAction" prefWidth="54.0" text="Voir tous les utilisateurs" StackPane.alignment="TOP_CENTER" />
    <Button onAction="#showAllUserNotationsButtonAction" prefWidth="54.0" text="Voir toutes les notations" StackPane.alignment="TOP_RIGHT" />
    <Button onAction="#showAllCoachNotationsButtonAction" prefWidth="54.0" text="Voir toutes les notations" StackPane.alignment="TOP_LEFT" />
    <Label fx:id="welcomeLabel" alignment="CENTER" prefHeight="80.0" prefWidth="296.0" style="-fx-font-size: 20px; -fx-font-weight: bold;" />
    <Button onAction="#AddNotationButtonAction" prefWidth="54.0" text="Noter un coach" StackPane.alignment="BOTTOM_LEFT" />
    <Button onAction="#ModifyNotationButtonAction" prefWidth="54.0" text="Modifier une notation" StackPane.alignment="BOTTOM_RIGHT" />
    <Button onAction="#logoutButtonAction" prefWidth="54.0" text="Logout" StackPane.alignment="BOTTOM_CENTER" />
</StackPane>
